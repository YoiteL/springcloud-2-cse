spring:
  application:
    name: eureka-consumer-feign
    
server:
  port: 7093

eureka:
  client:
    serviceUrl:
      defaultZone: http://localhost:7071/eureka/

feign:
  client:
    config:
      default:
        connectTimeout: 5000
        readTimeout: 5000

logging:
  file: ${spring.application.name}.log

service-provider:
  ribbon:
    NIWSServerListClassName: org.apache.servicecomb.springboot.starter.discovery.ServiceCombServerList